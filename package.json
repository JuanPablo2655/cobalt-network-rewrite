  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "bufferutil": "^4.0.5",
    "diff": "^5.0.0",
    "discord.js": "^13.3.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "glob": "^7.1.7",
    "ioredis": "^4.27.9",
