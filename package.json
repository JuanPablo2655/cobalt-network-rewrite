{
  "name": "cobalt-network-rewrite",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/Cobalt.js",
  "imports": {
    "#lib/*": "./dist/lib/*.js",
    "#lib/typings": "./dist/lib/typings/index.js",
    "#lib/data": "./dist/lib/data/index.js",
    "#lib/errors": "./dist/lib/errors/index.js",
    "#lib/structures": "./dist/lib/structures/index.js",
    "#lib/structures/commands": "./dist/lib/structures/commands/index.js",
    "#lib/structures/listeners": "./dist/lib/structures/listeners/index.js",
    "#root/*": "./dist/*.js",
    "#utils/*": "./dist/lib/utils/*.js",
    "#utils/common": "./dist/lib/utils/common/index.js",
    "#utils/functions": "./dist/lib/utils/functions/index.js"
  },
  "scripts": {
    "test": "jest",
    "test:ci": "yarn test --verbose --noStackTrace",
    "test:typescript": "tsc -b src",
    "test:prettier": "prettier --check .",
    "clean": "node scripts/build/clean.mjs",
    "build": "tsc -b src",
    "start": "NODE_ENV=production node --enable-source-maps dist/Cobalt.js",
    "start:dev": "node --enable-source-maps dist/Cobalt.js | pino-pretty -i pid,hostname -t yyyy-mm-dd HH:MM:ss",
    "deploy:commands": "node --enable-source-maps dist/deploy.js",
    "dev": "yarn build && yarn start:dev",
    "prettier": "prettier --write .",
    "lint": "eslint --fix --ext ts src tests",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JuanPablo2655/cobalt-network-rewrite.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JuanPablo2655/cobalt-network-rewrite/issues"
  },
  "homepage": "https://github.com/JuanPablo2655/cobalt-network-rewrite#readme",
  "dependencies": {
    "@discordjs/rest": "^0.4.1",
    "@elastic/ecs-pino-format": "^1.3.0",
    "@sapphire/time-utilities": "^1.7.4",
    "@sapphire/utilities": "^3.6.2",
    "bufferutil": "^4.0.6",
    "diff": "^5.1.0",
    "discord.js": "^14.0.0-dev.1655597371-3a77ce0",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "glob": "^8.0.1",
    "ioredis": "^5.0.6",
    "mongoose": "^6.4.0",
    "node-fetch": "2.6.7",
    "pino": "^7.10.0",
    "pino-elasticsearch": "^6.2.0",
    "pino-multi-stream": "^6.0.0",
    "pretty-ms": "^7.0.1",
    "prom-client": "^14.0.1",
    "utf-8-validate": "^5.0.9",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-angular": "^16.3.0",
    "@types/diff": "^5.0.2",
    "@types/express": "^4.17.13",
    "@types/glob": "^7.1.3",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.24",
    "@types/node-fetch": "^2.6.1",
    "@types/pino-multi-stream": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.23.0",
    "cloc": "^2.9.0",
    "discord-api-types": "^0.33.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "pino-pretty": "^7.6.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "ts-node-dev": "^1.1.6",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": "^16.0.0",
    "npm": "^7.0.0"
  }
}
