    "cloc": "^2.8.0",
    "discord-api-types": "^0.24.0",
    "jest": "^27.0.6",
    "prettier": "2.4.1",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^1.1.6",
    "tslib": "^2.3.1",
