  "description": "",
  "main": "./dist/cobalt.js",
  "imports": {
    "#lib/*": "./dist/lib/*.js",
    "#lib/data": "./dist/lib/data/index.js",
    "#lib/structures": "./dist/lib/structures/index.js",
    "#lib/structures/commands": "./dist/lib/structures/commands/index.js",
    "#lib/structures/events": "./dist/lib/structures/events/index.js",
    "#root/*": "./dist/*.js",
    "#utils/*": "./dist/lib/utils/*.js"
  },
  "scripts": {
    "test": "jest --collect-coverage",
    "test:ci": "yarn test --verbose --noStackTrace",
    "test:typescript": "tsc -b src",
    "test:prettier": "prettier --check .",
    "clean": "rm -rf dist",
    "build": "tsc -b src",
    "start": "node --enable-source-maps dist/cobalt.js",
    "deploy:commands": "node --enable-source-maps dist/deploy.js",
    "dev": "yarn build && yarn start",
    "prettier": "prettier --write ."
  },
  "repository": {
    "type": "git",
