{
  "name": "cobalt-network-rewrite",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/Cobalt.js",
  "type": "module",
  "imports": {
    "#lib/*": "./dist/lib/*.js",
    "#lib/typings": "./dist/lib/typings/index.js",
    "#lib/data": "./dist/lib/data/index.js",
    "#lib/errors": "./dist/lib/errors/index.js",
    "#lib/database": "./dist/lib/database/index.js",
    "#lib/structures": "./dist/lib/structures/index.js",
    "#lib/structures/commands": "./dist/lib/structures/commands/index.js",
    "#lib/structures/listeners": "./dist/lib/structures/listeners/index.js",
    "#root/*": "./dist/*.js",
    "#utils/*": "./dist/lib/utils/*.js",
    "#utils/common": "./dist/lib/utils/common/index.js",
    "#utils/functions": "./dist/lib/utils/functions/index.js",
    "#utils/resolvers": "./dist/lib/utils/resolvers/index.js"
  },
  "scripts": {
    "test": "vitest run",
    "test:typescript": "tsc -b src",
    "test:prettier": "prettier --check .",
    "clean": "node scripts/build/clean.mjs",
    "build": "tsc -b src",
    "start": "NODE_ENV=production node --enable-source-maps dist/Cobalt.js",
    "start:dev": "node --enable-source-maps dist/Cobalt.js | pino-pretty -i pid,hostname -t yyyy-mm-dd HH:MM:ss",
    "deploy:commands": "node --enable-source-maps dist/deploy.js",
    "dev": "yarn build && yarn start:dev",
    "prettier": "prettier --write .",
    "lint": "eslint --fix --ext ts src tests",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JuanPablo2655/cobalt-network-rewrite.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JuanPablo2655/cobalt-network-rewrite/issues"
  },
  "homepage": "https://github.com/JuanPablo2655/cobalt-network-rewrite#readme",
  "dependencies": {
    "@elastic/ecs-pino-format": "^1.3.0",
    "@prisma/client": "^4.8.0",
    "@sapphire/discord-utilities": "^2.12.0",
    "@sapphire/time-utilities": "^1.7.8",
    "@sapphire/utilities": "^3.11.0",
    "bufferutil": "^4.0.7",
    "diff": "^5.1.0",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "globby": "^13.1.3",
    "ioredis": "^5.2.4",
    "node-fetch": "^3.3.0",
    "pino": "^8.8.0",
    "pino-elasticsearch": "^6.3.0",
    "pretty-ms": "^8.0.0",
    "prom-client": "^14.1.0",
    "utf-8-validate": "^5.0.10",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-angular": "^17.3.0",
    "@types/diff": "^5.0.2",
    "@types/express": "^4.17.15",
    "@types/glob": "^8.0.0",
    "@types/node": "^18.11.17",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@vitest/coverage-c8": "^0.26.0",
    "c8": "^7.12.0",
    "cloc": "^2.10.0",
    "discord-api-types": "^0.37.24",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "pino-pretty": "^9.1.1",
    "prettier": "^2.8.1",
    "prisma": "^4.8.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vitest": "^0.26.0"
  },
  "engines": {
    "node": ">=16.9.0",
    "npm": "^7.0.0"
  }
}
