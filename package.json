{
  "name": "cobalt-network-rewrite",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/Cobalt.js",
  "imports": {
    "#lib/*": "./dist/lib/*.js",
    "#lib/typings": "./dist/lib/typings/index.js",
    "#lib/data": "./dist/lib/data/index.js",
    "#lib/errors": "./dist/lib/errors/index.js",
    "#lib/structures": "./dist/lib/structures/index.js",
    "#lib/structures/commands": "./dist/lib/structures/commands/index.js",
    "#lib/structures/events": "./dist/lib/structures/events/index.js",
    "#root/*": "./dist/*.js",
    "#utils/*": "./dist/lib/utils/*.js",
    "#utils/common": "./dist/lib/utils/common/index.js"
  },
  "scripts": {
    "test": "jest --collect-coverage",
    "test:ci": "yarn test --verbose --noStackTrace",
    "test:typescript": "tsc -b src",
    "test:prettier": "prettier --check .",
    "clean": "node scripts/build/clean.mjs",
    "build": "tsc -b src",
    "start": "NODE_ENV=production node --enable-source-maps dist/Cobalt.js",
    "start:dev": "node --enable-source-maps dist/Cobalt.js | pino-pretty -i pid,hostname -t yyyy-mm-dd HH:MM:ss",
    "deploy:commands": "node --enable-source-maps dist/deploy.js",
    "dev": "yarn build && yarn start:dev",
    "prettier": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JuanPablo2655/cobalt-network-rewrite.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JuanPablo2655/cobalt-network-rewrite/issues"
  },
  "homepage": "https://github.com/JuanPablo2655/cobalt-network-rewrite#readme",
  "dependencies": {
    "@discordjs/rest": "^0.3.0",
    "@elastic/ecs-pino-format": "^1.3.0",
    "@sapphire/time-utilities": "^1.6.0",
    "bufferutil": "^4.0.6",
    "diff": "^5.0.0",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "glob": "^7.1.7",
    "ioredis": "^4.28.5",
    "mongoose": "^6.2.0",
    "node-fetch": "2.6.7",
    "pino": "^7.6.5",
    "pino-elasticsearch": "^6.2.0",
    "pino-multi-stream": "^6.0.0",
    "pretty-ms": "^7.0.1",
    "prom-client": "^14.0.1",
    "utf-8-validate": "^5.0.8",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@types/diff": "^5.0.2",
    "@types/express": "^4.17.13",
    "@types/glob": "^7.1.3",
    "@types/ioredis": "^4.28.7",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.15",
    "@types/node-fetch": "2.5.12",
    "@types/pino-multi-stream": "^5.1.3",
    "cloc": "^2.9.0",
    "discord-api-types": "^0.26.1",
    "jest": "^27.5.0",
    "pino-pretty": "^7.5.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.6",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": "^16.0.0",
    "npm": "^7.0.0"
  }
}
